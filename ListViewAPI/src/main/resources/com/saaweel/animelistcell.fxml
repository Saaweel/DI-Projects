<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.saaweel.AnimeCellController">
   <children>
      <Label fx:id="title" text="Label" textAlignment="CENTER" VBox.vgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="synopsis" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="400.0" text="Label" wrapText="true">
         <VBox.margin>
            <Insets left="50.0" right="50.0" />
         </VBox.margin></Label>
      <HBox alignment="TOP_CENTER">
         <children>
            <Label fx:id="startDate" text="Label" textFill="#636363">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="ageRating" text="Label" textFill="#760404">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="episodes" layoutX="264.0" layoutY="15.0" text="Label">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="showType" layoutX="284.0" layoutY="15.0" text="Label">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <ImageView fx:id="image" fitHeight="217.0" fitWidth="173.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</VBox>
