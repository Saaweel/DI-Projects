<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="486.0" prefWidth="393.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.saaweel.informaciondecontacto.Controller">
    <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
        <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="21.0" prefWidth="278.0" styleClass="titulo" text="Información de contacto" />
    <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
        <Label styleClass="label" text="Nombre: " />
        <TextField maxWidth="1.7976931348623157E308" styleClass="input" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="72.0" maxWidth="1.7976931348623157E308">
        <Label styleClass="label" text="Email: " />
        <TextField maxWidth="1.7976931348623157E308" styleClass="input" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="117.0" maxWidth="1.7976931348623157E308">
        <Label styleClass="label" text="Teléfono: " />
        <TextField maxWidth="1.7976931348623157E308" styleClass="input" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="163.0" maxWidth="1.7976931348623157E308">
        <Label styleClass="label" text="Fecha de Nacimiento: " />
        <DatePicker maxWidth="1.7976931348623157E308" styleClass="input" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox alignment="CENTER_LEFT" layoutX="30.0" layoutY="208.0" maxWidth="1.7976931348623157E308">
        <Label styleClass="label" text="Intereses: " />
        <ListView maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" styleClass="listView" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox alignment="CENTER" layoutX="30.0" layoutY="276.0" maxWidth="1.7976931348623157E308">
        <Button mnemonicParsing="false" text="Guardar">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
        </Button>
        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Cancelar">
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
        </Button>
    </HBox>
</VBox>
